          SQLTask:SqlStatementSource="/* Report fur musst have attributs */&#xA;&#xA;/*Basiselemente*/&#xA;&#xA;Insert into ReportingDimension(report,museURL,filmID) Select 'Basiselementfarbe /'+'BasiselementID:'+CONVERT(varchar(24),Basiselement.Basiselementid) as &#xA;report,museURL,kostuembasiselement.FilmID &#xA;from ((Basiselement INNER JOIN [kostuemex2].dbo.kostuembasiselement ON&#xA; Basiselement.BasiselementID = kostuembasiselement.BasiselementID) INNER JOIN Kostuem On &#xA; kostuembasiselement.FilmID =Kostuem.FilmID AND kostuembasiselement.RollenID = Kostuem.RollenID&#xA;  AND kostuembasiselement.KostuemID = Kostuem.KostuemID ) &#xA;  where NOT Exists (Select BasiselementID from Basiselementfarbe &#xA; where Basiselement.BasiselementID = Basiselementfarbe.BasiselementID)&#xA; GO&#xA; Insert into ReportingDimension(report,museURL,filmID) Select 'Basiselementmatrial /'+'BasiselementID:'+CONVERT(varchar(24),Basiselement.Basiselementid) as &#xA;report,museURL ,kostuembasiselement.FilmID&#xA;from ((Basiselement INNER JOIN [kostuemex2].dbo.kostuembasiselement ON&#xA; Basiselement.BasiselementID = kostuembasiselement.BasiselementID) INNER JOIN Kostuem On &#xA; kostuembasiselement.FilmID =Kostuem.FilmID AND kostuembasiselement.RollenID = Kostuem.RollenID&#xA;  AND kostuembasiselement.KostuemID = Kostuem.KostuemID ) &#xA;  where NOT Exists (Select BasiselementID from Basiselementmaterial &#xA; where Basiselement.BasiselementID = Basiselementmaterial.BasiselementID)&#xA;GO&#xA; Insert into ReportingDimension(report,museURL,filmID) Select 'Basiselementdesign /'+'BasiselementID:'+CONVERT(varchar(24),Basiselement.Basiselementid) as &#xA;report,museURL, kostuembasiselement.FilmID&#xA;from ((Basiselement INNER JOIN [kostuemex2].dbo.kostuembasiselement ON&#xA; Basiselement.BasiselementID = kostuembasiselement.BasiselementID) INNER JOIN Kostuem On &#xA; kostuembasiselement.FilmID =Kostuem.FilmID AND kostuembasiselement.RollenID = Kostuem.RollenID&#xA;  AND kostuembasiselement.KostuemID = Kostuem.KostuemID ) &#xA;  where NOT Exists (Select BasiselementID from Basiselementdesign&#xA; where Basiselement.BasiselementID = Basiselementdesign.BasiselementID)&#xA;GO&#xA;&#xA;/*Teilelement*/&#xA;Insert into ReportingDimension(report,museURL,filmID) Select 'Teilelementdesign /'+'TeilementID:'+CONVERT(varchar(24),Teilelemente.TeilelementID) as &#xA;report, museURL,kostuembasiselement.FilmID&#xA;from ((((Teilelemente INNER JOIN HelperBasisteilelement ON Teilelemente.TeilelementID=HelperBasisteilelement.TeilelementID)&#xA;INNER JOIN Basiselement ON HelperBasisteilelement.BasiselementID=Basiselement.BasiselementID)&#xA;INNER JOIN [kostuemex2].dbo.kostuembasiselement ON&#xA; Basiselement.BasiselementID = kostuembasiselement.BasiselementID) INNER JOIN Kostuem On &#xA; kostuembasiselement.FilmID =Kostuem.FilmID AND kostuembasiselement.RollenID = Kostuem.RollenID&#xA;  AND kostuembasiselement.KostuemID = Kostuem.KostuemID )&#xA;  where NOT Exists (Select TeilelementID from Teilelementdesign as a where &#xA;Teilelemente.TeilelementID = a.TeilelementID)&#xA;GO&#xA;Insert into ReportingDimension(report,museURL,filmID) Select 'Teilelementmatrial /'+'TeilementID:'+CONVERT(varchar(24),Teilelemente.TeilelementID) as &#xA;report,museURL, kostuembasiselement.FilmID&#xA;from ((((Teilelemente INNER JOIN HelperBasisteilelement ON Teilelemente.TeilelementID=HelperBasisteilelement.TeilelementID)&#xA;INNER JOIN Basiselement ON HelperBasisteilelement.BasiselementID=Basiselement.BasiselementID)&#xA;INNER JOIN [kostuemex2].dbo.kostuembasiselement ON&#xA; Basiselement.BasiselementID = kostuembasiselement.BasiselementID) INNER JOIN Kostuem On &#xA; kostuembasiselement.FilmID =Kostuem.FilmID AND kostuembasiselement.RollenID = Kostuem.RollenID&#xA;  AND kostuembasiselement.KostuemID = Kostuem.KostuemID )&#xA;  where NOT Exists (Select TeilelementID from Teilelementmaterial as a where &#xA;Teilelemente.TeilelementID = a.TeilelementID)&#xA;GO&#xA;Insert into ReportingDimension(report,museURL,filmID) Select 'Teilelementfarbe /'+'TeilementID:'+CONVERT(varchar(24),Teilelemente.TeilelementID) as &#xA;report, museURL,kostuembasiselement.FilmID&#xA;from ((((Teilelemente INNER JOIN HelperBasisteilelement ON Teilelemente.TeilelementID=HelperBasisteilelement.TeilelementID)&#xA;INNER JOIN Basiselement ON HelperBasisteilelement.BasiselementID=Basiselement.BasiselementID)&#xA;INNER JOIN [kostuemex2].dbo.kostuembasiselement ON&#xA; Basiselement.BasiselementID = kostuembasiselement.BasiselementID) INNER JOIN Kostuem On &#xA; kostuembasiselement.FilmID =Kostuem.FilmID AND kostuembasiselement.RollenID = Kostuem.RollenID&#xA;  AND kostuembasiselement.KostuemID = Kostuem.KostuemID )&#xA;  where NOT Exists (Select TeilelementID from Teilelementfarbe as a where &#xA;Teilelemente.TeilelementID = a.TeilelementID)&#xA;GO&#xA;/* Kostueme */ &#xA;&#xA;Insert into ReportingDimension(report,museURL,filmID) Select ('KostuemCharaktereigenschaften, '+'FilmID:'+CONVERT(varchar(12),FilmID)+',RollenID:'+CONVERT(varchar(12),RollenID)+',KostuemID:'+CONVERT(varchar(12),KostuemID))&#xA;as report, museURL,Kostuem.FilmID from Kostuem where NOT Exists(Select *from Kostuemcharaktereigenschaft where &#xA;Kostuemcharaktereigenschaft.FilmID = Kostuem.FilmID AND Kostuemcharaktereigenschaft.RollenID= Kostuem.RollenID AND Kostuemcharaktereigenschaft.KostuemID = Kostuem.KostuemID) &#xA;Order By FilmID,RollenID,KostuemID ASC&#xA;&#xA;Insert into ReportingDimension(report,museURL,filmID) Select ('KostuemTageszeit, '+'FilmID:'+CONVERT(varchar(12),FilmID)+',RollenID:'+CONVERT(varchar(12),RollenID)+',KostuemID:'+CONVERT(varchar(12),KostuemID)) as &#xA;report, museURL,Kostuem.FilmID from Kostuem where NOT Exists(Select *from Kostuemtageszeit where &#xA;Kostuemtageszeit.FilmID = Kostuem.FilmID AND Kostuemtageszeit.RollenID= Kostuem.RollenID AND Kostuemtageszeit.KostuemID = Kostuem.KostuemID) Order By FilmID,RollenID,KostuemID ASC&#xA;&#xA;Insert into ReportingDimension(report,museURL,filmID) Select ('KostuemSpielort, '+'FilmID:'+CONVERT(varchar(12),FilmID)+',RollenID:'+CONVERT(varchar(12),RollenID)+',KostuemID:'+CONVERT(varchar(12),KostuemID)) as &#xA;report, museURL,Kostuem.FilmID from Kostuem where NOT Exists(Select *from kostuemspielort where &#xA;kostuemspielort.FilmID = Kostuem.FilmID AND kostuemspielort.RollenID= Kostuem.RollenID AND kostuemspielort.KostuemID = Kostuem.KostuemID) Order By FilmID,RollenID,KostuemID ASC&#xA;&#xA;&#xA;Insert into ReportingDimension(report,museURL,filmID)  Select ('KostuemAlterseindruck, '+'FilmID:'+CONVERT(varchar(12),FilmID)+',RollenID:'+CONVERT(varchar(12),RollenID)+',KostuemID:'+CONVERT(varchar(12),KostuemID)) as report, museURL,Kostuem.FilmID from &#xA;Kostuem where NOT Exists(Select *from Kostuemalterseindruck where &#xA;Kostuemalterseindruck.FilmID = Kostuem.FilmID AND Kostuemalterseindruck.RollenID= Kostuem.RollenID AND Kostuemalterseindruck.KostuemID = Kostuem.KostuemID) Order By FilmID,RollenID,KostuemID ASC&#xA;&#xA;Insert into ReportingDimension(report,museURL,filmID) Select ('KostuemSpielzeit, '+'FilmID:'+CONVERT(varchar(12),FilmID)+',RollenID:'+CONVERT(varchar(12),RollenID)+',KostuemID:'+CONVERT(varchar(12),KostuemID)) as report, museURL,Kostuem.FilmID &#xA;from Kostuem where NOT Exists(Select *from kostuemspielzeit where &#xA;kostuemspielzeit.FilmID = Kostuem.FilmID AND kostuemspielzeit.RollenID= Kostuem.RollenID AND kostuemspielzeit.KostuemID = Kostuem.KostuemID) Order By FilmID,RollenID,KostuemID ASC&#xA;&#xA;&#xA;Insert into ReportingDimension(report,museURL,filmID) Select ('Kostuemtimecode/nicht vorhanden, '+'FilmID:'+CONVERT(varchar(12),FilmID)+',RollenID:'+CONVERT(varchar(12),RollenID)+',KostuemID:'+CONVERT(varchar(12),KostuemID)) as report,museURL, Kostuem.FilmID &#xA;from Kostuem where NOT Exists&#xA;(Select *from Faktentabellekostuemtimecode where &#xA;Faktentabellekostuemtimecode.FilmID = Kostuem.FilmID AND Faktentabellekostuemtimecode.RollenID = Kostuem.RollenID AND &#xA;Faktentabellekostuemtimecode.KostuemID = Kostuem.KostuemID) Order By FilmID,RollenID,KostuemID ASC&#xA;&#xA;Insert into ReportingDimension(report,filmID) Select ('Kostuemtimecode/ungültige Spieldauer, '+'FilmID:'+CONVERT(varchar(12),FilmID)+&#xA;',RollenID:'+CONVERT(varchar(12),RollenID)+',KostuemID:'+CONVERT(varchar(12),KostuemID) +',Spieldauer: '+CONVERT(varchar(12),Spieldauer)) as report,Faktentabellekostuemtimecode.FilmID&#xA;from Faktentabellekostuemtimecode  where Spieldauer like '-%' or Spieldauer &gt; 900&#xA;&#xA;/* Rollen */&#xA;&#xA;Insert into ReportingDimension(report,museURL,filmID) Select ('Rollenstereotyp, '+'FilmID:'+CONVERT(varchar(12),FilmID)+', S.Vorname: '+Schauspielervorname + ', S.Nachname: '+Schauspielernachname) as &#xA;report,museURL,Rolle.FilmID from Rolle where NOT Exists&#xA;(Select *from Mapping_Stereotyp where Mapping_Stereotyp.FilmID = Rolle.FilmID AND Mapping_Stereotyp.RollenID = Rolle.RollenID) Order By FilmID,RollenID ASC&#xA;&#xA;Insert into ReportingDimension(report,museURL,filmID)  Select ('RollenDominateCharaktereigenschaft, '+'FilmID:'+CONVERT(varchar(12),FilmID)+ ', S.Vorname: '+Schauspielervorname + ', S.Nachname: '+Schauspielernachname) as &#xA;report, museURL,Rolle.FilmID from Rolle where NOT Exists (Select *from Rolledominantecharaktereigenschaft where Rolledominantecharaktereigenschaft.FilmID = Rolle.FilmID AND &#xA;Rolledominantecharaktereigenschaft.RollenID= Rolle.RollenID) Order By FilmID,RollenID ASC&#xA;&#xA;Insert into ReportingDimension(report,museURL,filmID) Select ('RollenFamilenstand, '+'FilmID:'+CONVERT(varchar(12),FilmID) +', S.Vorname: '+Schauspielervorname + ', S.Nachname: '+Schauspielernachname) as &#xA;report,museURL,Rolle.FilmID from Rolle where NOT Exists (Select *from Rollenfamilienstand where Rollenfamilienstand.FilmID = Rolle.FilmID AND Rollenfamilienstand.RollenID= Rolle.RollenID) Order By FilmID,RollenID ASC" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
