            string conString = "Server=localhost;Database=kostuemDWX3;Trusted_Connection=True";
            SqlDataReader reader;

            try
            {
                SqlConnection connectionReader = new SqlConnection(conString);
                connectionReader.Open();

                SqlCommand commandReader = new SqlCommand();
                commandReader.Connection = connectionReader;
                commandReader.CommandText = "Select * From Rolle";
                reader = commandReader.ExecuteReader();

                SqlConnection connectionUpdater = new SqlConnection(conString);
                connectionUpdater.Open();

                while (reader.Read())
                {
                    string data = reader["RollenID"] + " " + reader["FilmID"] + " " + reader["Stereotyp"];

                    string s = reader["Stereotyp"].ToString();
                    string[] stereotyps = { "NULL" };

                    if (s != "")
                    {

                        if (s.Contains(","))
                        {
                            stereotyps = s.Split(',');
                        }
                        else if (s.Contains(";"))
                        {
                            stereotyps = s.Split(';');
                        }
                        else
                        {
                            stereotyps[0] = s;

                        }


                        for (int i = 0; stereotyps.Length > i; i++)
                        {

                            SqlCommand commandUpdate = new SqlCommand();
                            commandUpdate.Connection = connectionUpdater;
                            String sTyp = stereotyps[i].Trim();

                            commandUpdate.CommandText = "Select Count(*) From Stereotyp Where Name = @name";
                            commandUpdate.Parameters.Add("@name", SqlDbType.NVarChar);
                            commandUpdate.Parameters["@name"].Value = sTyp;

                            if (Convert.ToInt32(commandUpdate.ExecuteScalar()) == 0)
                            {

                                commandUpdate.CommandText = "Insert into Stereotyp (Name) Values (@nameIn)";
                                commandUpdate.Parameters.Add("@nameIn", SqlDbType.NVarChar);
                                commandUpdate.Parameters["@nameIn"].Value = sTyp;
                                commandUpdate.ExecuteNonQuery();

                            }

                            commandUpdate.CommandText = "Select StereotypID From Stereotyp Where name = @nameID";
                            commandUpdate.Parameters.Add("@nameID", SqlDbType.NVarChar);
                            commandUpdate.Parameters["@nameID"].Value = sTyp;
                            int stereotypID = Convert.ToInt32(commandUpdate.ExecuteScalar());

                            commandUpdate.CommandText = "Insert into Mapping_Stereotyp (RollenID,FilmID,StereotypID) Values (@RollenID, @FilmID, @StereotypID)";
                            commandUpdate.Parameters.Add("@RollenID", SqlDbType.Int);
                            commandUpdate.Parameters.Add("@FilmID", SqlDbType.Int);
                            commandUpdate.Parameters.Add("@StereotypID", SqlDbType.Int);

                            commandUpdate.Parameters["@RollenID"].Value = reader["RollenID"];
                            commandUpdate.Parameters["@FilmID"].Value = reader["FilmID"];
                            commandUpdate.Parameters["@StereotypID"].Value = stereotypID;
                            commandUpdate.ExecuteNonQuery();

                            Console.WriteLine(sTyp);

                        }

                    }

                }

                connectionReader.Close();
                connectionUpdater.Close();


            }
            catch (Exception e)
            {
                Console.Write(e.ToString());
            }

			Dts.TaskResult = (int)ScriptResults.Success;
