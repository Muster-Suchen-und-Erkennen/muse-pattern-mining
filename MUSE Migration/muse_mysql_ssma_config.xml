<?xml version="1.0" encoding="utf-8"?>
<variables
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="C:\Microsoft SQL Server Migration Assistant for MySQL\Schemas\ConsoleScriptVariablesSchema.xsd">

	<variable name="$WorkingFolder$"  value="."/>

	<variable-group name="MySqlParams">
		<variable-group name="MySqlConnectionParams">
			<variable name="$SourceServer$" value="muse.informatik.uni-stuttgart.de"/>
			<variable name="$SourcePort$" value="3306"/>
			<variable name="$SourceUserName$" value="RepoMigration"/>
			<variable name="$SourcePassword$" value="RepoMigration"/>
		</variable-group>

		<variable-group name="MySqlServerObjectParams">
			<variable name="$SourceDatabase$" value="`KostuemRepo`"/>
		</variable-group>
	</variable-group>
	
	<variable-group name="SQLServerParams">
		<variable-group name="SqlServerConnectionParams">
			<variable name="$TargetServerName$" value="localhost"/>
			<variable name="$TargetDB$" value="muse_nightly"/>
		</variable-group>    
	</variable-group>
	
	<variable-group name="ProjectSpecs">
		<variable name="$project_name$" value="MUSE Migration"/>
		<variable name="$project_overwrite$" value="true"/>
		<variable name="$project_folder$" value="$WorkingFolder$\$project_name$"/>
	</variable-group>
	
	<variable-group name="Reports">
		<variable name="$SummaryReportsFolder$" value="$WorkingFolder$"/>
	</variable-group>
	
</variables>
