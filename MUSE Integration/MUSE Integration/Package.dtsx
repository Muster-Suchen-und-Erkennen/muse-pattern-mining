    /// </summary>
    /// <param name="Row">The row that is currently passing through the component</param>
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        int i = 0;
        int caseNumber = (int) Math.Round(Row.Anteil * 10);
        while (i < caseNumber)
        {
            Output0Buffer.AddRow();
            Output0Buffer.RollenID = Row.RollenID;
            Output0Buffer.FilmID = Row.FilmID;
            Output0Buffer.KostuemID = Row.KostuemID;
            Output0Buffer.DominanteFarbe = Row.DominanteFarbe;
            Output0Buffer.DominanteFunktion = Row.DominanteFunktion;
            Output0Buffer.DominanterZustand = Row.DominanterZustand;
            Output0Buffer.Kostuemkurztext = Row.Kostuemkurztext;
            Output0Buffer.MuseURL = Row.MuseURL;
            Output0Buffer.Ortsbegebenheit = Row.Ortsbegebenheit;
            Output0Buffer.StereotypRelevant = Row.StereotypRelevant;
            {
                uint blobLength = Row.Szenenbeschreibung.Length;
                byte[] blobBytes = Row.Szenenbeschreibung.GetBlobData(0, (int)blobLength);
                Output0Buffer.Szenenbeschreibung.AddBlobData(blobBytes);
            }
            Output0Buffer.DominanteFarbeL1 = Row.DominanteFarbeL1;
            Output0Buffer.DominanteFarbeL2 = Row.DominanteFarbeL2;
            Output0Buffer.DominanteFarbeL3 = Row.DominanteFarbeL3;
            Output0Buffer.DominanterZustandL1 = Row.DominanterZustandL1;
            Output0Buffer.DominanterZustandL2 = Row.DominanterZustandL2;
            Output0Buffer.DominanterZustandL3 = Row.DominanterZustandL3;
            ++i;
        }
    }
