                Name="TreeNode.cs"
                Encoding="UTF8"><![CDATA[using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ST_1ebb46fb9d2b4c5abed465bdb863318a
{
    /// <summary>
    /// Simple Tree Structure
    /// </summary>
    /// <typeparam name="T"></typeparam>
    class TreeNode<T>
    {
        public LinkedList<TreeNode<T>> Children
        {
            get;
            private set;
        }

        public T Value;

        public TreeNode<T> Parent
        {
            get;
            set;
        }

        private delegate Tx Reducer<Tx>(TreeNode<T> node, Tx current);

        private Tx Reduce<Tx>(Reducer<Tx> reducer, Tx initial)
        {
            Tx result = initial;
            foreach (TreeNode<T> node in Children)
            {
                result = reducer(node, result);
            }
            return result;
        }

        public TreeNode(T data)
        {
            this.Value = data;
            Children = new LinkedList<TreeNode<T>>();
        }

        public void InsertChild(TreeNode<T> child)
        {
            Children.AddLast(child);
            child.Parent = this;
        }

        public void RemoveChild(TreeNode<T> child)
        {
            Children.Remove(child);
        }

        public bool Empty()
        {
            return Children.Count == 0;
        }

        public TreeNode<T> FindChild(T search, bool recursive)
        {
            if (Value.Equals(search))
            {
                return this;
            }
            return Reduce<TreeNode<T>>(
                delegate(TreeNode<T> node, TreeNode<T> current)
                {
                    if (current == null)
                    {
                        if (node.Value.Equals(search))
                        {
                            return node;
                        }
                        else if (recursive)
                        {
                            return node.FindChild(search, recursive);
                        }
                    }
                    return current;
                }, null);
        }

        public int MaxDepth()
        {
            return Reduce<int>(
                delegate(TreeNode<T> node, int current)
                {
                    int depth = (node.Children.Count == 0) ? 1 : node.MaxDepth();
                    return Math.Max(depth, current);
                }, 0) + 1;
        }

        public int Level()
        {
            int result = 1;
            TreeNode<T> node = Parent;
            while (node != null)
            {
                result++;
                node = node.Parent;
            }
            return result;
        }

    }
}]]></ProjectItem>
