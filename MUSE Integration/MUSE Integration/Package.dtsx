              SQLTask:SqlStatementSource="IF OBJECT_ID('dbo.reporting_invalid_filmdauer', 'V') IS NOT NULL&#xA;  DROP VIEW [dbo].[reporting_invalid_filmdauer]&#xA;GO&#xA;CREATE VIEW [dbo].[reporting_invalid_filmdauer] AS&#xA;SELECT F.FilmID, F.Filmtitel, F.Originaltitel, F.Dauer&#xA;FROM Film F&#xA;WHERE F.Dauer = NULL OR F.Dauer &lt; 1" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
