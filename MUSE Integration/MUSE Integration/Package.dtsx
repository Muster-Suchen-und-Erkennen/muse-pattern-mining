                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT kostuem.FilmID, kostuem.RollenID, kostuem.KostuemID
  , SUM(DATEDIFF([second], kostuemtimecode.Timecodeanfang, kostuemtimecode.Timecodeende)) AS 'Sekunden'
  , film.Dauer * 60 as 'Filmdauer'
  , ROUND((CAST(SUM(DATEDIFF([second], kostuemtimecode.Timecodeanfang, kostuemtimecode.Timecodeende)) AS float) * 100 / (film.Dauer * 60)), 2) as 'Anteil'
FROM kostuemtimecode
INNER JOIN kostuem
ON kostuemtimecode.KostuemID = kostuem.KostuemID
AND kostuemtimecode.RollenID = kostuem.RollenID
AND kostuemtimecode.FilmID   = kostuem.FilmID
INNER JOIN film
ON kostuem.FilmID = film.FilmID
WHERE DATEDIFF([second], kostuemtimecode.Timecodeanfang, kostuemtimecode.Timecodeende) &gt; 0
GROUP BY kostuem.FilmID, kostuem.RollenID, kostuem.KostuemID, film.Dauer
ORDER BY kostuem.FilmID, kostuem.RollenID, kostuem.KostuemID</property>
