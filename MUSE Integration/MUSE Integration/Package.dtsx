         *  object rawConnection = Dts.Connections["Prices.zip"].AcquireConnection(Dts.Transaction);
         *  string filePath = (string)rawConnection;
         *  //Use the connection in some code here, then release the connection
         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion

        const String DbConnectionNameSource = "muse";
        const String DbConnectionNameTarget = "muse_analytics";

        private OleDbConnection connectionSource;
        private OleDbConnection connectionTarget;

        /// <summary>
        /// Get the connection from the SSIS package
        /// </summary>
        private void AquireConnections()
        {
            connectionSource = new OleDbConnection(Dts.Connections[DbConnectionNameSource].ConnectionString);
            connectionSource.Open();
            Debug.Assert(connectionSource != null);
            connectionTarget = new OleDbConnection(Dts.Connections[DbConnectionNameTarget].ConnectionString);
            connectionTarget.Open();
            Debug.Assert(connectionTarget != null);
        }

        /// <summary>
        /// Close an existing connection
        /// </summary>
        private void ReleaseConnections()
        {
            if (connectionSource != null)
            {
                connectionSource.Close();
            }
            if (connectionTarget != null)
            {
                connectionTarget.Close();
            }
        }

        /// <summary>
        /// Fetch the tree structure for the given table
        /// </summary>
        /// <param name="table"></param>
        /// <param name="columnParent"></param>
        /// <param name="columnChild"></param>
        /// <returns></returns>
        private TreeNode<String> FetchTree(String table, String columnParent, String columnChild)
        {
            String query = String.Format("SELECT * FROM {0} WHERE {1} <> ''", table, columnParent);
            OleDbCommand stmt = new OleDbCommand(query, connectionSource);
            OleDbDataReader reader = stmt.ExecuteReader();
            TreeNode<String> tree = new TreeNode<string>("");
            try
            {
                while (reader.Read())
                {
                    String parentValue = reader[columnParent].ToString();
                    String childValue = reader[columnChild].ToString();
                    TreeNode<String> child = null;
                    if (!childValue.Equals(""))
                    {
                        child = tree.FindChild(childValue, true);

                    }
                    bool hasChild = (child != null);
                    if (!hasChild && !childValue.Equals(""))
                    {
                        child = new TreeNode<String>(childValue);
                    }
                    TreeNode<String> parent = tree.FindChild(parentValue, true);
                    bool hasParent = (parent != null);
                    if (!hasParent)
                    {
                        parent = new TreeNode<string>(parentValue);
                    }
                    if (hasChild)
                    {
                        child.Parent.RemoveChild(child);
                        if (!hasParent)
                        {
                            child.Parent.InsertChild(parent);
                        }
                    }
                    if (parent != null && child != null)
                    {
                        parent.InsertChild(child);
                    }
                    if (!hasParent)
                    {
                        tree.InsertChild(parent);
                    }
                }
            }
            finally
            {
                reader.Close();
            }
            return tree;
        }

        /// <summary>
        /// Add hierarchy columns to the given table
        /// </summary>
        /// <param name="table">Table name</param>
        /// <param name="prefix">Prefix, used for the hierarchy column names</param>
        /// <param name="count">amouint of hierarchy columns to create</param>
        private void AddColumns(String table, String prefix, int count)
        {
            String query = String.Format("ALTER TABLE {0} ADD ", table);
            for (int i = count; i > 0; --i)
            {
                query += String.Format("[{0}_L{1}] nvarchar(45), ", prefix, i);
            }
            query = query.TrimEnd(' ', ',');
            OleDbCommand stmt = new OleDbCommand(query, connectionTarget);
            try
            {
                stmt.ExecuteNonQuery();
            }
            catch (Exception e)
            {
            }
        }

        /// <summary>
        /// Fill in the hierarchy columns
        /// </summary>
        /// <param name="table">Table name</param>
        /// <param name="columnValue">Name of the column that holds the actual value</param>
        /// <param name="prefix">Prefix, used for the hierarchy columns</param>
        /// <param name="tree">The hierarchy tree</param>
        private void FillHierachyColumns(String table, String columnValue, String prefix, TreeNode<String> tree)
        {
            OleDbCommand stmt = new OleDbCommand(String.Format("SELECT DISTINCT {1} FROM {0}", table, columnValue), connectionTarget);
            OleDbDataReader reader = stmt.ExecuteReader();
            try
            {
                int depth = tree.MaxDepth() - 1;
                while (reader.Read())
                {
                    String query = String.Format("UPDATE {0} SET ", table);
                    String value = reader[columnValue].ToString();
                    TreeNode<String> node = tree.FindChild(value, true);
                    if (node != null)
                    {
                        int level = node.Level() - 1;
                        int i = depth;
                        while (i >= level)
                        {
                            query += String.Format("[{0}_L{1}]='{2}', ", new object[] { prefix, i, value });
                            --i;
                        }
                        TreeNode<String> parent = node.Parent;
                        while (parent != null && i > 0)
                        {
                            query += String.Format("[{0}_L{1}]='{2}', ", new object[] { prefix, i, parent.Value });
                            --i;
                            parent = parent.Parent;
                        }
                    }
                    query = query.TrimEnd(' ', ',');
                    query += String.Format(" WHERE [{0}]='{1}'", columnValue, value);
                    OleDbCommand command = new OleDbCommand(query, connectionTarget);
                    command.ExecuteNonQuery();
                }
                
            }
            finally
            {
                reader.Close();
            }
        }

        /// <summary>
        /// Operation data structure
        /// </summary>
        struct Operation
        {
            public String TableName;
            public String ColumnValue;
            public String DomainTableName;
            public String DomainColumnValue;
            public String DomainColumnChild;
        };

        private Operation CreateOperation(String tableName, String columnValue, String domainTableName, String domainColumnValue, String domainColumnChild)
        {
            Operation result = new Operation();
            result.TableName = tableName;
            result.ColumnValue = columnValue;
            result.DomainTableName = domainTableName;
            result.DomainColumnValue = domainColumnValue;
            result.DomainColumnChild = domainColumnChild;
            return result;
        }

		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
        {
            AquireConnections();

            Operation[] operations = new Operation[] {
                CreateOperation("basiselementfarbe", "Farbname", "farbendomaene", "Farbname", "UntergeordneteFarbe"),
                CreateOperation("basiselementzustand", "Zustandsname", "zustandsdomaene", "Zustandsname", "UntergeordneterZustand"),
                CreateOperation("filmgenre", "Genre", "genredomaene", "Genrename", "UntergeordnetesGenre"),
                CreateOperation("kostuemalterseindruck", "Alterseindruck", "alterseindruckdomaene", "Alterseindruckname", "UntergeordneterAlterseindruck"),
                CreateOperation("basiselement", "Basiselementname", "basiselementdomaene", "Basiselementname", "UntergeordnetesBasiselement"),
                // CreateOperation("charakter", "Charaktereigenschaft", "charaktereigenschaftsdomaene", "Charaktereigenschaftname", "UntergeordneteCharaktereigenschaft"),
                CreateOperation("kostuemcharaktereigenschaft", "Charaktereigenschaft", "charaktereigenschaftsdomaene", "Charaktereigenschaftname", "UntergeordneteCharaktereigenschaft"),
                CreateOperation("rolledominantecharaktereigenschaft", "DominanteCharaktereigenschaft", "typusdomaene", "Typusname", "UntergeordneterTypus"),
                CreateOperation("basiselementdesign", "Designname", "designdomaene", "Designname", "UntergeordnetesDesign"),
                CreateOperation("filmfarbkonzept", "Farbkonzept", "farbkonzeptdomaene", "Farbkonzeptname", "UntergeordnetesFarbkonzept"),
                CreateOperation("basiselementform", "Formname", "formendomaene", "Formname", "UntergeordneteForm"),
                CreateOperation("basiselementfunktion", "Funktionsname", "funktionsdomaene", "Funktionsname", "UntergeordneteFunktion"),
                CreateOperation("kostuemkoerpermodifikation", "Koerpermodifikationname", "koerpermodifikationsdomaene", "Koerpermodifikationsname", "UntergeordneteKoerpermodifikation"),
                CreateOperation("basiselementmaterial", "Materialname", "materialdomaene", "Materialname", "UntergeordnetesMaterial"),
                CreateOperation("filmproduktionsort", "Produktionsort", "produktionsortdomaene", "Produktionsortname", "UntergeordneterProduktionsort"),
                CreateOperation("kostuemspielort", "Spielort", "spielortdomaene", "Spielortname", "UntergeordneterSpielort"),
                CreateOperation("kostuemspielzeit", "Spielzeit", "spielzeitdomaene", "Spielzeitname", "UntergeordneteSpielzeit"),
                CreateOperation("kostuemtageszeit", "Tageszeit", "tageszeitdomaene", "Tageszeit", "UntergeordneteTageszeit"),
                CreateOperation("basiselementtrageweise", "Trageweisename", "trageweisendomaene", "Trageweisenname", "UntergeordneteTrageweise")
            };

            int progress = 0;
            bool fireAgain = true;
            Dts.Events.FireProgress("Starting to flatten hierarchies", 0, 0, 0, "Hierarchy", ref fireAgain);
            object progressMutex = new object();
            List<System.Threading.ManualResetEvent> completedEvents = new List<System.Threading.ManualResetEvent>();
            foreach (Operation op in operations)
            {
                System.Threading.ManualResetEvent completedEvent = new System.Threading.ManualResetEvent(false);
                System.Threading.ThreadPool.QueueUserWorkItem(
                    new System.Threading.WaitCallback(
                        delegate(Object state)
                        {
                            TreeNode<String> tree = FetchTree(op.DomainTableName, op.DomainColumnValue, op.DomainColumnChild);
                            AddColumns(op.TableName, op.ColumnValue, tree.MaxDepth() - 1);
                            FillHierachyColumns(op.TableName, op.ColumnValue, op.ColumnValue, tree);
                            lock (progressMutex)
                            {
                                ++progress;
                                int percentage = (int)(((double)progress / (double)operations.Length) * 100.0);
                                Dts.Events.FireProgress(String.Format("Completed flattening hierarchy for '{0}'", op.TableName), percentage, 0, 0, "Hierarchy", ref fireAgain);
                            }
                            completedEvent.Set();
                        }));
                completedEvents.Add(completedEvent);
            }
            System.Threading.WaitHandle.WaitAll(completedEvents.ToArray());
            Dts.Events.FireProgress("Completed flattening hierarchies", 100, 0, 0, "Hierarchy", ref fireAgain);
            
            Dts.TaskResult = (int)ScriptResults.Success;
            ReleaseConnections();
        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>
