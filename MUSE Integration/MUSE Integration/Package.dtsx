          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{82D75573-97DA-4285-AB7B-6708F7CF9820}"
              SQLTask:SqlStatementSource="IF OBJECT_ID('dbo.reporting_invalid_timecodes', 'V') IS NOT NULL&#xA;  DROP VIEW dbo.reporting_invalid_timecodes&#xA;GO&#xA;CREATE VIEW dbo.reporting_invalid_timecodes AS&#xA;SELECT F.Filmtitel&#xA;, K.Kostuemkurztext&#xA;, R.Rollenvorname&#xA;, R.Rollennachname&#xA;, DATEDIFF(second, KT.Timecodeanfang, KT.Timecodeende) as 'Sekunden im Bild'&#xA;, K.MuseURL&#xA;FROM Kostuem K, Kostuemtimecode KT, Film F, Rolle R&#xA;WHERE &#xA;    KT.KostuemID = K.KostuemID&#xA;AND KT.RollenID = K.RollenID&#xA;AND KT.FilmID = K.FilmID&#xA;AND F.FilmID = K.FilmID&#xA;AND R.FilmID = F.FilmID&#xA;AND R.RollenID = K.RollenID&#xA;AND DATEDIFF(second, KT.Timecodeanfang, KT.Timecodeende) &lt; 1" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
