         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion

        const String DbConnectionNameSource = "muse";
        const String DbConnectionNameTarget = "muse_analytics";

        private OleDbConnection connectionSource;
        private OleDbConnection connectionTarget;

        /// <summary>
        /// Get the connection from the SSIS package
        /// </summary>
        private void AquireConnections()
        {
            connectionSource = new OleDbConnection(Dts.Connections[DbConnectionNameSource].ConnectionString);
            connectionSource.Open();
            Debug.Assert(connectionSource != null);
            connectionTarget = new OleDbConnection(Dts.Connections[DbConnectionNameTarget].ConnectionString);
            connectionTarget.Open();
            Debug.Assert(connectionTarget != null);
        }

        /// <summary>
        /// Close an existing connection
        /// </summary>
        private void ReleaseConnections()
        {
            if (connectionSource != null)
            {
                connectionSource.Close();
            }
            if (connectionTarget != null)
            {
                connectionTarget.Close();
            }
        }

        /// <summary>
        /// Fetch the tree structure for the given table
        /// </summary>
        /// <param name="table"></param>
        /// <param name="columnValue"></param>
        /// <param name="columnParent"></param>
        /// <returns></returns>
        private TreeNode<String> FetchTree(String table, String columnValue, String columnParent)
        {
            String query = String.Format("SELECT * FROM {0} WHERE {1} <> '' AND {2} <> ''", table, columnValue, columnParent);
            OleDbCommand stmt = new OleDbCommand(query, connectionSource);
            OleDbDataReader reader = stmt.ExecuteReader();
            TreeNode<String> tree = new TreeNode<string>("");
            try
            {
                while (reader.Read())
                {
                    String actualValue = reader[columnValue].ToString();
                    String parentValue = reader[columnParent].ToString();
                    TreeNode<String> parent = null;
                    TreeNode<String> node = null;
                    parent = tree.FindChild(parentValue, true);
                    if (parent == null)
                    {
                        parent = new TreeNode<string>(parentValue);
                        tree.InsertChild(parent);
                    }
                    node = tree.FindChild(actualValue, true);
                    if (node != null)
                    {
                        node.Parent.RemoveChild(node);
                    }
                    else
                    {
                        node = new TreeNode<string>(actualValue);
                    }
                    parent.InsertChild(node);
                }
            }
            finally
            {
                reader.Close();
            }
            if (tree.Children.Count > 0)
            {
                tree = tree.Children.First.Value;
                tree.Parent = null;
            }
            return tree;
        }

        /// <summary>
        /// Add hierarchy columns to the given table
        /// </summary>
        /// <param name="table">Table name</param>
        /// <param name="prefix">Prefix, used for the hierarchy column names</param>
        /// <param name="count">amouint of hierarchy columns to create</param>
        private void AddColumns(String table, String prefix, int count)
        {
            String query = String.Format("ALTER TABLE {0} ADD ", table);
            for (int i = (count - 1); i > 0; --i)
            {
                query += String.Format("[{0}_L{1}] nvarchar(45), ", prefix, i);
            }
            query = query.TrimEnd(' ', ',');
            OleDbCommand stmt = new OleDbCommand(query, connectionTarget);
            try
            {
                stmt.ExecuteNonQuery();
            }
            catch (Exception e)
            {
            }
        }

        /// <summary>
        /// Fill in the hierarchy columns
        /// </summary>
        /// <param name="table">Table name</param>
        /// <param name="columnValue">Name of the column that holds the actual value</param>
        /// <param name="prefix">Prefix, used for the hierarchy columns</param>
        /// <param name="tree">The hierarchy tree</param>
        private void FillHierachyColumns(String table, String columnValue, String prefix, TreeNode<String> tree)
        {
            OleDbCommand stmt = new OleDbCommand(String.Format("SELECT DISTINCT {1} FROM {0}", table, columnValue), connectionTarget);
            OleDbDataReader reader = stmt.ExecuteReader();
            try
            {
                int depth = tree.MaxDepth();
                while (reader.Read())
                {
                    String query = String.Format("UPDATE {0} SET ", table);
                    String value = reader[columnValue].ToString();
                    TreeNode<String> node = tree.FindChild(value, true);
                    if (node != null)
                    {
                        int level = node.Level();
                        if (level <= 1)
                        {
                            continue;
                        }
                        int i = depth;
                        while (i >= level)
                        {
                            query += String.Format("[{0}_L{1}]='{2}', ", new object[] { prefix, i - 1, value });
                            --i;
                        }
                        TreeNode<String> parent = node.Parent;
                        while (parent != null && i > 1)
                        {
                            query += String.Format("[{0}_L{1}]='{2}', ", new object[] { prefix, i - 1, parent.Value });
                            --i;
                            parent = parent.Parent;
                        }
                        query = query.TrimEnd(' ', ',');
                        query += String.Format(" WHERE [{0}]='{1}'", columnValue, value);
                        OleDbCommand command = new OleDbCommand(query, connectionTarget);
                        command.ExecuteNonQuery();
                    }
                }
            }
            finally
            {
                reader.Close();
            }
        }

        /// <summary>
        /// Operation data structure
        /// </summary>
        struct Operation
        {
            public String TableName;
            public String ColumnValue;
            public String DomainTableName;
            public String DomainColumnValue;
            public String DomainColumnParent;
        };

        private Operation CreateOperation(String tableName, String columnValue, String domainTableName, String domainColumnValue, String domainColumnParent)
        {
            Operation result = new Operation();
            result.TableName = tableName;
            result.ColumnValue = columnValue;
            result.DomainTableName = domainTableName;
            result.DomainColumnValue = domainColumnValue;
            result.DomainColumnParent = domainColumnParent;
            return result;
        }

		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
        {
            AquireConnections();

            Operation[] operations = new Operation[] {
                CreateOperation("basiselement", "Basiselementname", "basiselementdomaene", "BasiselementName", "UebergeordnetesElement"),
                CreateOperation("basiselementdesign", "Designname", "designdomaene", "DesignName", "UebergeordnetesElement"),
                CreateOperation("basiselementfarbe", "Farbname", "farbendomaene", "FarbName", "UebergeordnetesElement"),
                CreateOperation("basiselementform", "Formname", "formendomaene", "FormName", "UebergeordnetesElement"),
                CreateOperation("basiselementfunktion", "Funktionsname", "funktionsdomaene", "FunktionsName", "UebergeordnetesElement"),
                CreateOperation("basiselementmaterial", "Materialname", "materialdomaene", "MaterialName", "UebergeordnetesElement"),
                CreateOperation("basiselementtrageweise", "Trageweisename", "trageweisendomaene", "TrageweisenName", "UebergeordnetesElement"),
                CreateOperation("basiselementzustand", "Zustandsname", "zustandsdomaene", "ZustandsName", "UebergeordnetesElement"),
                CreateOperation("filmgenre", "Genre", "genredomaene", "GenreName", "UebergeordnetesElement"),
                CreateOperation("filmfarbkonzept", "Farbkonzept", "farbkonzeptdomaene", "FarbkonzeptName", "UebergeordnetesElement"),
                CreateOperation("filmproduktionsort", "Produktionsort", "produktionsortdomaene", "ProduktionsortName", "UebergeordnetesElement"),
                CreateOperation("kostuem", "DominanteFarbe", "farbendomaene", "FarbName", "UebergeordnetesElement"),
                CreateOperation("kostuem", "DominanteFunktion", "funktionsdomaene", "FunktionsName", "UebergeordnetesElement"),
                CreateOperation("kostuem", "DominanterZustand", "zustandsdomaene", "ZustandsName", "UebergeordnetesElement"),
                CreateOperation("kostuemalterseindruck", "Alterseindruck", "alterseindruckdomaene", "AlterseindruckName", "UebergeordnetesElement"),
                CreateOperation("kostuemcharaktereigenschaft", "Charaktereigenschaft", "charaktereigenschaftsdomaene", "CharaktereigenschaftsName", "UebergeordnetesElement"),
                CreateOperation("kostuemkoerpermodifikation", "Koerpermodifikationname", "koerpermodifikationsdomaene", "KoerpermodifikationsName", "UebergeordnetesElement"),
                CreateOperation("kostuemspielort", "Spielort", "spielortdomaene", "SpielortName", "UebergeordnetesElement"),
                CreateOperation("kostuemspielort", "SpielortDetail", "spielortdetaildomaene", "SpielortDetailName", "UebergeordnetesElement"),
                CreateOperation("kostuemspielzeit", "Spielzeit", "spielzeitdomaene", "SpielzeitName", "UebergeordnetesElement"),
                CreateOperation("kostuemtageszeit", "Tageszeit", "tageszeitdomaene", "TageszeitName", "UebergeordnetesElement"),
                CreateOperation("rolle", "Rollenberuf", "rollenberufdomaene", "RollenberufName", "UebergeordnetesElement"),
                CreateOperation("rolledominantecharaktereigenschaft", "DominanteCharaktereigenschaft", "typusdomaene", "TypusName", "UebergeordnetesElement"),
                CreateOperation("rollestereotyp", "Stereotyp", "stereotypdomaene", "StereotypName", "UebergeordnetesElement"),
            };

            int progress = 0;
            bool fireAgain = true;
            Dts.Events.FireProgress("Starting to flatten hierarchies", 0, 0, 0, "Hierarchy", ref fireAgain);
            object progressMutex = new object();
            List<System.Threading.ManualResetEvent> completedEvents = new List<System.Threading.ManualResetEvent>();
            foreach (Operation op in operations)
            {
                // FIXME: Disabled threading since we have to update tables multiple times
                //System.Threading.ManualResetEvent completedEvent = new System.Threading.ManualResetEvent(false);
                //System.Threading.ThreadPool.QueueUserWorkItem(
                //    new System.Threading.WaitCallback(
                //        delegate(Object state)
                //        {
                            TreeNode<String> tree = FetchTree(op.DomainTableName, op.DomainColumnValue, op.DomainColumnParent);
                            AddColumns(op.TableName, op.ColumnValue, tree.MaxDepth());
                            FillHierachyColumns(op.TableName, op.ColumnValue, op.ColumnValue, tree);
                            lock (progressMutex)
                            {
                                ++progress;
                                int percentage = (int)(((double)progress / (double)operations.Length) * 100.0);
                                Dts.Events.FireProgress(String.Format("Completed flattening hierarchy for '{0}'", op.TableName), percentage, 0, 0, "Hierarchy", ref fireAgain);
                            }
                            //completedEvent.Set();
                //        }));
                //completedEvents.Add(completedEvent);
            }
            //System.Threading.WaitHandle.WaitAll(completedEvents.ToArray());
            Dts.Events.FireProgress("Completed flattening hierarchies", 100, 0, 0, "Hierarchy", ref fireAgain);
            
            Dts.TaskResult = (int)ScriptResults.Success;
            ReleaseConnections();
        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
