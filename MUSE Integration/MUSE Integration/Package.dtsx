    /// <typeparam name="T"></typeparam>
    class TreeNode<T>
    {
        public LinkedList<TreeNode<T>> Children
        {
            get;
            private set;
        }

        public T Value;

        public TreeNode<T> Parent
        {
            get;
            set;
        }

        private delegate Tx Reducer<Tx>(TreeNode<T> node, Tx current);

        private Tx Reduce<Tx>(Reducer<Tx> reducer, Tx initial)
        {
            Tx result = initial;
            foreach (TreeNode<T> node in Children)
            {
                result = reducer(node, result);
            }
            return result;
        }

        public TreeNode(T data)
        {
            this.Value = data;
            Children = new LinkedList<TreeNode<T>>();
        }

        public void InsertChild(TreeNode<T> child)
        {
            Children.AddLast(child);
            child.Parent = this;
        }

        public void RemoveChild(TreeNode<T> child)
        {
            Children.Remove(child);
        }

        public bool Empty()
        {
            return Children.Count == 0;
        }

