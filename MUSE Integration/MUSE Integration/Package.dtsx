                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        basiselement.BasiselementID, SUM(DATEDIFF([second], kostuemtimecode.Timecodeanfang, kostuemtimecode.Timecodeende)) AS 'Sekunden', film.Dauer * 60 AS 'Filmdauer', 
                         ROUND((CAST(SUM(DATEDIFF([second], kostuemtimecode.Timecodeanfang, kostuemtimecode.Timecodeende)) AS float) * 100) / (film.Dauer * 60), 2) AS 'Anteil', rolle.Rollenrelevanz
FROM            kostuembasiselement INNER JOIN
                         basiselement ON kostuembasiselement.BasiselementID = basiselement.BasiselementID INNER JOIN
                         kostuemtimecode INNER JOIN
                         kostuem ON kostuemtimecode.KostuemID = kostuem.KostuemID AND kostuemtimecode.RollenID = kostuem.RollenID AND kostuemtimecode.FilmID = kostuem.FilmID INNER JOIN
                         film ON kostuem.FilmID = film.FilmID ON kostuembasiselement.KostuemID = kostuem.KostuemID AND kostuembasiselement.RollenID = kostuem.RollenID AND 
                         kostuembasiselement.FilmID = kostuem.FilmID INNER JOIN
                         rolle ON kostuem.RollenID = rolle.RollenID AND kostuem.FilmID = rolle.FilmID
WHERE        (DATEDIFF([second], kostuemtimecode.Timecodeanfang, kostuemtimecode.Timecodeende) &gt; 0) AND (film.Dauer IS NOT NULL) AND (film.Dauer &gt; 0)
GROUP BY kostuem.FilmID, kostuem.RollenID, kostuem.KostuemID, basiselement.BasiselementID, film.Dauer, rolle.Rollenrelevanz
ORDER BY kostuem.FilmID, kostuem.RollenID, kostuem.KostuemID</property>
